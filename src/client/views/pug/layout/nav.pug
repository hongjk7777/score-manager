nav.navbar.navbar-light.bg-light.border-bottom.shadow-sm
  .container-fluid
    a#logo.ms-1.navbar-brand(href='#')
        img(src="/public/views/img/logo.png", alt="HNI 해냄 학원", srcset="" width='140') 
    .me-2
        if(authentication === false) 
            a#login-btn.btn.btn-primary.btn-sm(href='/login') 로그인
            //- a#signup-btn.btn.btn-outline-primary.ms-3(href='/signup') 회원가입
        else 
            if (user)
                if(user.username === 'admin')
                    #user-menu.dropdown
                        button#dropdownMenuButton.btn.btn-secondary.btn-sm.dropdown-toggle(type='button' data-bs-toggle='dropdown' aria-expanded='false')
                            img(src="https://identicon-api.herokuapp.com/hongjk7777/25?format=png" alt="avatar image")
                        .dropdown-menu.dropdown-menu-end(aria-labelledby='dropdownMenuButton')
                            li
                                a.dropdown-item(href='/classList') 내 클래스
                            li
                                a.dropdown-item(href='/settings') 비밀번호 변경

                            li
                                form(action="/logout", method="post") 
                                    button.dropdown-item(type="submit") 로그아웃    
                else 
                    #user-menu.dropdown
                        button#dropdownMenuButton.btn.btn-secondary.btn-sm.dropdown-toggle(type='button' data-bs-toggle='dropdown' aria-expanded='false')
                            img(src="https://identicon-api.herokuapp.com/hongjk7777/25?format=png" alt="avatar image")
                        .dropdown-menu.dropdown-menu-end(aria-labelledby='dropdownMenuButton')
                            li
                                a.dropdown-item(href='/class') 시험 성적
                            li
                                a.dropdown-item(href='/settings') 비밀번호 변경

                            li
                                form(action="/logout", method="post") 
                                    button.dropdown-item(type="submit") 로그아웃

            else 
                #user-menu.dropdown
                    button#dropdownMenuButton.btn.btn-secondary.dropdown-toggle(type='button' data-bs-toggle='dropdown' aria-expanded='false')
                        img(src="https://identicon-api.herokuapp.com/hongjk7777/25?format=png" alt="avatar image")
                    .dropdown-menu.dropdown-menu-end(aria-labelledby='dropdownMenuButton')
                        li
                            a.dropdown-item(href='/settings') 비밀번호 변경
                        li
                            a.dropdown-item(href='/class') 시험 성적
                        li
                            form(action="/logout", method="post") 
                                button.dropdown-item(type="submit") 로그아웃


